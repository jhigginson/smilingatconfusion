<ul>
<% @artists.each do |artist| %>
  <li><%= link_to artist.name, artist %>
      <%= link_to("Edit", edit_artist_path(artist)) %>
      <%= link_to "Delete", artist_path(artist),
            method: :delete,
            data: { confirm: "Are you sure you want to delete this artist?"
                  } %>
  </li>
<% end %>
</ul>
<br>
<%= link_to "New Artist", new_artist_path %>
