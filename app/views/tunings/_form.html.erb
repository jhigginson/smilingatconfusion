<div class="new_form_div" id="new_tuning_form_div">
  <%= link_to "", class: 'close-div' do %>
    <i class="icon-remove"></i>
  <% end %>
  <%= simple_form_for @tuning, remote: true, id: :new_tuning do |f| %>
